@Entity
@Table(name = "T_DC_PAY_TYPE")
public class DcPayTypeEntity {

	@Id
	private Long id;

	@Column(name = "PAY_TYPE_CODE")
	private String code;

	@Column(name = "PAY_TYPE_NAME")
	private String name;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
