CREATE TABLE "LIFE".t_market_card_gift_product (
  "ID" NUMBER(12) NOT NULL,
  card_id NUMBER(12) NOT NULL,
  product_id NUMBER(12),
  product_name VARCHAR2(150 BYTE),
  PRIMARY KEY ("ID")
);
COMMENT ON TABLE "LIFE".t_market_card_gift_product IS '礼金商品范围表';
COMMENT ON COLUMN "LIFE".t_market_card_gift_product."ID" IS '主键';
COMMENT ON COLUMN "LIFE".t_market_card_gift_product.card_id IS '礼金批次id';
COMMENT ON COLUMN "LIFE".t_market_card_gift_product.product_id IS '商品编号';
COMMENT ON COLUMN "LIFE".t_market_card_gift_product.product_name IS '商品名称';