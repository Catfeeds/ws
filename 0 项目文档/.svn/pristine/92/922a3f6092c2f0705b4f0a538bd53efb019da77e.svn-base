CREATE TABLE "LIFE".t_bsct_terminal (
  pos_id VARCHAR2(100 BYTE) NOT NULL,
  shop_id VARCHAR2(20 BYTE),
  pos_type NUMBER(2),
  pos_vendors VARCHAR2(200 BYTE),
  pos_class NUMBER(2),
  use_status NUMBER(2),
  "ID" NUMBER(9)
);
COMMENT ON COLUMN "LIFE".t_bsct_terminal.pos_class IS '1：硬件终端  2：软pos  3：星网锐捷pos';
COMMENT ON COLUMN "LIFE".t_bsct_terminal.use_status IS '1：在用 0：停用';