CREATE TABLE "LIFE".t_web_page (
  "ID" NUMBER(8) NOT NULL,
  code VARCHAR2(50 BYTE),
  title VARCHAR2(100 BYTE),
  des VARCHAR2(300 BYTE),
  create_user NUMBER(9),
  create_time VARCHAR2(14 BYTE),
  update_user NUMBER(9),
  update_time VARCHAR2(14 BYTE),
  CONSTRAINT navipage_id PRIMARY KEY ("ID")
);
COMMENT ON TABLE "LIFE".t_web_page IS '网页信息表';
COMMENT ON COLUMN "LIFE".t_web_page.code IS '代码';
COMMENT ON COLUMN "LIFE".t_web_page.title IS '标题';
COMMENT ON COLUMN "LIFE".t_web_page.des IS '描述';
COMMENT ON COLUMN "LIFE".t_web_page.create_user IS '创建人';
COMMENT ON COLUMN "LIFE".t_web_page.create_time IS '创建时间';
COMMENT ON COLUMN "LIFE".t_web_page.update_user IS '修改人';
COMMENT ON COLUMN "LIFE".t_web_page.update_time IS '修改时间';