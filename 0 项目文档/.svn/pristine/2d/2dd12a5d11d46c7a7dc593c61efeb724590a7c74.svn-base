CREATE TABLE "LIFE".t_supplier_apply (
  "ID" NUMBER(9) NOT NULL,
  supplier_name VARCHAR2(200 BYTE),
  business_scope VARCHAR2(200 BYTE),
  address VARCHAR2(200 BYTE),
  mobile VARCHAR2(40 BYTE),
  phone VARCHAR2(40 BYTE),
  contact VARCHAR2(50 BYTE),
  status NUMBER(1),
  create_time VARCHAR2(14 BYTE),
  update_user VARCHAR2(20 BYTE),
  update_time VARCHAR2(14 BYTE),
  CONSTRAINT t_supplier_apply_pk PRIMARY KEY ("ID")
);
COMMENT ON COLUMN "LIFE".t_supplier_apply."ID" IS '主键';
COMMENT ON COLUMN "LIFE".t_supplier_apply.supplier_name IS '供应商名称';
COMMENT ON COLUMN "LIFE".t_supplier_apply.business_scope IS '经营范围';
COMMENT ON COLUMN "LIFE".t_supplier_apply.address IS '商户所在地';
COMMENT ON COLUMN "LIFE".t_supplier_apply.mobile IS '手机号';
COMMENT ON COLUMN "LIFE".t_supplier_apply.phone IS '座机';
COMMENT ON COLUMN "LIFE".t_supplier_apply.contact IS '联系人';
COMMENT ON COLUMN "LIFE".t_supplier_apply.status IS '状态0是正常，-1是删除';
COMMENT ON COLUMN "LIFE".t_supplier_apply.create_time IS '申请时间';
COMMENT ON COLUMN "LIFE".t_supplier_apply.update_user IS '修改人';
COMMENT ON COLUMN "LIFE".t_supplier_apply.update_time IS '修改时间';