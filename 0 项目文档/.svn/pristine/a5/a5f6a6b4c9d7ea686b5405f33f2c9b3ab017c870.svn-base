CREATE TABLE "LIFE".t_market_zero_activity_code (
  "ID" NUMBER(8) NOT NULL,
  mobile VARCHAR2(15 BYTE) NOT NULL,
  code NUMBER(20) NOT NULL,
  create_time VARCHAR2(30 BYTE),
  "TYPE" VARCHAR2(5 BYTE),
  goods_id NUMBER(8) NOT NULL,
  activity_id NUMBER(8),
  PRIMARY KEY ("ID")
);
COMMENT ON COLUMN "LIFE".t_market_zero_activity_code.activity_id IS '活动ID';