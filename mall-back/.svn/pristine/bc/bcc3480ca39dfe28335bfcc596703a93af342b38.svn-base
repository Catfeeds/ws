 *           修改： 增加param1属性，对应数据库中的param1字段，用来区分此彩信是普通彩信还是大头贴，“1” 表示大头贴
 */

public class TsdMmsBoxinfo implements java.io.Serializable {

	/**
	 * 地区码
	 */
	private String areaCode;

	/**
	 * 内容
	 */
	private String content;

	/**
	 * 创建人
	 */
	private String createUserId;

	/**
	 * 主键id
	 */
	private Long id;

	/**
	 * 彩信还是大头贴，“1” 表示大头贴
	 */

	private String param1 = "";

	/**
	 * 手机号码
	 */
	private String terminalId;

	/**
	 * 标题
	 */
	private String title;

	/**
	 * 类型
	 */
	private Long typeId;

	/**
	 * 更新时间
	 */
	private String updateTime;

	/**
	 * 默认构造函数
	 */
	public TsdMmsBoxinfo() {
	}

	/**
	 * 完整构造函数
	 * 
	 * @param typeId
	 *            类型
	 * @param terminalId
	 *            电话号码
	 * @param title
	 *            标题
	 * @param content
	 *            内容
	 * @param updateTime
	 *            更新时间
	 * @param areaCode
	 *            地区码
	 */
	public TsdMmsBoxinfo(Long typeId, String terminalId, String title, String content, String updateTime, String areaCode) {
		this.typeId = typeId;
		this.terminalId = terminalId;
		this.title = title;
		this.content = content;
		this.updateTime = updateTime;
		this.areaCode = areaCode;
	}

	/**
	 * 获取 areaCode
	 * 
	 * @return areaCode
	 */
	public String getAreaCode() {
		return areaCode;
	}

	/**
	 * 获取 content
	 * 
	 * @return content
	 */
	public String getContent() {
		return content;
	}

	/**
	 * 获取 createUserId
	 * 
	 * @return createUserId
	 */
	public String getCreateUserId() {
		return createUserId;
	}

	/**
	 * 获取 id
	 * 
	 * @return id
	 */
	public Long getId() {
		return id;
	}

	public String getParam1() {
		return param1;
	}

	/**
	 * 获取 terminalId
	 * 
	 * @return terminalId
	 */
	public String getTerminalId() {
		return terminalId;
	}

	/**
	 * 获取 title
	 * 
	 * @return title
	 */
	public String getTitle() {
		return title;
	}

	/**
	 * 获取 typeId
	 * 
	 * @return typeId
	 */
	public Long getTypeId() {
		return typeId;
	}

	/**
	 * 获取 updateTime
	 * 
	 * @return updateTime
	 */
	public String getUpdateTime() {
		return updateTime;
	}

	/**
	 * 设置 areaCode
	 * 
	 * @param areaCode
	 *            areaCode
	 */
	public void setAreaCode(String areaCode) {
		this.areaCode = areaCode;
	}

	/**
	 * 设置 content
	 * 
	 * @param content
	 *            content
	 */
	public void setContent(String content) {
		this.content = content;
	}

	/**
	 * 设置 createUserId
	 * 
	 * @param createUserId
	 *            createUserId
	 */
	public void setCreateUserId(String createUserId) {
		this.createUserId = createUserId;
	}

	/**
	 * 设置 id
	 * 
	 * @param id
	 *            id
	 */
	public void setId(Long id) {
		this.id = id;
	}

	public void setParam1(String param1) {
		this.param1 = param1;
	}

	/**
	 * 设置 terminalId
	 * 
	 * @param terminalId
	 *            terminalId
	 */
	public void setTerminalId(String terminalId) {
		this.terminalId = terminalId;
	}

	/**
	 * 设置 title
	 * 
	 * @param title
	 *            title
	 */
	public void setTitle(String title) {
		this.title = title;
	}

	/**
	 * 设置 typeId
	 * 
	 * @param typeId
	 *            typeId
	 */
	public void setTypeId(Long typeId) {
		this.typeId = typeId;
	}

	/**
	 * 设置 updateTime
	 * 
	 * @param updateTime
	 *            updateTime
	 */
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}

}