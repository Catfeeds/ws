@Entity
@Table(name = "T_SYS_spcode")
public class SysSpcode extends IdEntity {

	private String spcode;

	private Integer valid;

	@Column(name = "valid")
	public Integer getValid() {
		return valid;
	}

	public void setValid(Integer valid) {
		this.valid = valid;
	}

	@Column(name = "sp_code")
	public String getSpcode() {
		return spcode;
	}

	public void setSpcode(String spcode) {
		this.spcode = spcode;
	}

}
