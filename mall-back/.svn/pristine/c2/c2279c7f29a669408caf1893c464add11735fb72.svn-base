db.jdbcUrl=jdbc:oracle:thin:@192.168.1.14:1521:orcl
#db.user=encrypted:062172
db.user=life
#db.password=encrypted:5A70311BB4A8
db.password=lifepass

# 池初始化大小
db.initialSize=1

# 池最小连接数
db.minIdle=1

# 池最大连接数
db.maxActive=5

# 获取连接等待超时的时间
db.maxWait=60000

# 配置间隔多久才进行一次检测，检测需要关闭的空闲连接，单位是毫秒
db.timeBetweenEvictionRunsMillis=60000

# 一个连接在池中最小生存的时间，单位是毫秒
db.minEvictableIdleTimeMillis=300000

# 是否打开PSCache
db.poolPreparedStatements=false

# 指定每个连接上PSCache的大小
db.maxPoolPreparedStatementPerConnectionSize=20
